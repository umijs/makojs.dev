    <script>
      function getFirstVisibleHeaderAnchor() {
        const headerAnchors = document.querySelectorAll('article h2 a.header-anchor');
        for (let i = 0; i < headerAnchors.length; i++) {
          if (headerAnchors[i].parentNode.getBoundingClientRect().top > 0) {
            return headerAnchors[i];
          }
        }
        return null;
      }
      function getLinkNav() {
        for (let i = 0; i < navs.length; i++) {
          if (navs[i].getAttribute('href') === location.pathname) {
            return navs[i];
          }
        }
      }
      let navs = document.querySelectorAll('nav ul ul li a');
      let currentId = null;
      function highlightNav() {
        const linkNav = getLinkNav();
        if (linkNav) {
          linkNav.style.fontWeight = 'bold';
          return;
        }
        const headerAnchor = getFirstVisibleHeaderAnchor();
        if (!headerAnchor) {
          return;
        }
        let id = location.pathname + headerAnchor.getAttribute('href');
        if (currentId === id) {
          return;
        }
        navs.forEach((nav) => {
          if (nav.getAttribute('href') === id) {
            nav.style.fontWeight = 'bold';
          } else {
            nav.style.fontWeight = 'normal';
          }
        });
        currentId = id;
      }
      window.addEventListener('scroll', highlightNav);
      highlightNav();
    </script>
